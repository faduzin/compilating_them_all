/* Programa de teste para C- */
int x;
int v[5];

int soma(int n) {
    int i;
    int s;
    i = 0;
    s = 0;
    while (i < n) {
        s = s + v[i];
        i = i + 1;
    }
    return s;
}

void main(void) {
    int i;
    i = 0;
    while (i < 5) {
        v[i] = i + 1;
        i = i + 1;
    }

    x = soma(5);

    if (x == 15) {
        x = x + 1;
    } else {
        x = x - 1;
    }

    return;
}
